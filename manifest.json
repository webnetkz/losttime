{
    "manifest_version": 2,
    "name": "Lost Time My",
    "version": "1.0",
    
    "icons": {
      "128": "losttime/logo.png"
    },
    "content_scripts": [
      {
        "matches": [ "<all_urls>" ],
        "js": [ "losttime/content.js" ]
      }
    ],

    "permissions": [
      "https://google.com/*"
    ],
  
        "browser_action": {
          "default_title": "LostTime",
          "default_icon": "losttime/logo.png",
          "default_popup": "losttime/popup.html"
      }
      
  }