{
    "manifest_version": 2,
    "name": "Lost Time",
    "version": "1.0",
    
    "icons": {
      "128": ""
    },
    "content_scripts": [
      {
        "matches": [ "*://*/*" ],
        "js": [ "content.js" ]
      }
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "permissions": [
      "http://losttime.su/*"
    ],
  
        "browser_action": {
          "default_title": "LostTime",
          "default_icon": "",
          "default_popup": "popup.html"
      }
      
  }